{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@atoms/*": ["src/components/atoms/*"],
      "@molecules/*": ["src/components/molecules/*"],
      "@screens/*": ["src/screens/*"],
      "@navigator/*": ["src/navigator/*"],
      "@services/*": ["src/services/*"],
      "@models/*": ["src/models/*"],
      "@data/*": ["src/data/*"],
      "@store/*": ["src/store/*"]
    }
  }
}
